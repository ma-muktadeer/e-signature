	<parameter name="TX_EMPLOYEE_ID" class="java.lang.String"/>
	<parameter name="ID_INSTITUTION_KEY" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT ROWNUM AS SL, V.* 
FROM (
    SELECT I.TX_NAME, 
           I.TX_PA, 
           I.TX_DELIGATION,
           CASE 
               WHEN I.TX_SIGNATURE_STATUS = 'IN_ACTIVE' THEN 'Inactive' 
               WHEN I.TX_SIGNATURE_STATUS = 'ACTIVE' THEN 'Active'
               WHEN I.TX_SIGNATURE_STATUS = 'CANCELED' THEN 'Cancelled'
               ELSE I.TX_SIGNATURE_STATUS 
           END AS TX_SIGNATURE_STATUS,
           I.TX_SIGNATURE_PATH 
    FROM VW_SIGNATURE_INFO I 
    WHERE I.TX_EMPLOYEE_ID = NVL( $P{TX_EMPLOYEE_ID}, I.TX_EMPLOYEE_ID) 
      AND I.IS_MAIN_SIGNATURE = 1 
      AND I.IS_ACTIVE = 1
      AND I.ID_INSTITUTION_KEY = $P{ID_INSTITUTION_KEY}
) V]]>
	</queryString>
