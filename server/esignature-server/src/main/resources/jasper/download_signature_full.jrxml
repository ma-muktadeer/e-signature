	<queryString language="SQL">
		<![CDATA[SELECT ROWNUM AS SL, V.TX_NAME, V.TX_PA, V.TX_SIGNATURE_STATUS, V.TX_SIGNATURE_PATH, V.TX_DELIGATION
FROM (
    SELECT I.TX_NAME, 
           I.TX_PA, 
           I.TX_DELIGATION,
           CASE 
               WHEN I.TX_SIGNATURE_STATUS = 'IN_ACTIVE' THEN 'Inactive' 
               WHEN I.TX_SIGNATURE_STATUS = 'ACTIVE' THEN 'Active'
               WHEN I.TX_SIGNATURE_STATUS = 'CANCELED' THEN 'Cancelled'
               ELSE I.TX_SIGNATURE_STATUS 
           END AS TX_SIGNATURE_STATUS,
           I.TX_SIGNATURE_PATH 
    FROM VW_SIGNATURE_INFO I 
    WHERE I.TX_STATUS = 'APPROVED' 
        AND INSTR(UPPER(I.INSTITUTION_NAME), 'PRIME BANK', 1, 1) = 1
        AND I.IS_MAIN_SIGNATURE = 1 
        AND I.IS_ACTIVE = 1
) V]]>
	</queryString>
